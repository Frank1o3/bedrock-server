                    "last_played_world_size_label": {
                      "type": "label",
                      "text": "#local_worldfile_size",
                      "shadow": true,
                      "color": "$main_header_text_color",
                      "bindings": [
                        {
                          "binding_name": "#local_worldfile_size",
                          "binding_type": "collection",
                          "binding_collection_name": "local_worlds"
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "world_game_mode_label": {
          "type": "label",
