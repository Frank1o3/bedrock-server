                  {
                    "last_played_date_label": {
                      "type": "label",
                      "text": "#local_world_date",
                      "shadow": true,
                      "color": "$main_header_text_color",
                      "bindings": [
                        {
                          "binding_name": "#local_world_date",
                          "binding_type": "collection",
                          "binding_collection_name": "local_worlds"
                        }
                      ]
                    }
                  }
                ]
              }
            },
            {
              "last_played_world_size": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [ "100%c", 10 ],
                "controls": [
                  {
                    "padding_left@common.empty_panel": {
                      "size": [ 1, 0 ]
                    }
                  },
                  {
                    "last_played_world_size_header": {
                      "type": "label",
                      "text": "playscreen.worldSize",
                      "shadow": true,
                      "color": "$main_header_text_color"
                    }
                  },
                  {
                    "padding_0@common.empty_panel": {
                      "size": [ 4, 0 ]
                    }
