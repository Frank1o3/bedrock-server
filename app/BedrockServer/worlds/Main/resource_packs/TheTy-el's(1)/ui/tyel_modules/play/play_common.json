  "local_recent_world_item_content": {
    "type": "panel",
    "controls": [
      {
        "focus_border_image@common.non_interact_focus_border_button": {
          "size": [ "100%", "100%" ],
          "focus_enabled": false,
          "tts_inherit_siblings": true,
          "$default_focus_border_control": "common.focus_border_white",
          "$hover_focus_border_control": "common.focus_border_white",
          "$pressed_focus_border_control": "common.focus_border_white",
          "$content_inside": "tyel_play_common.local_recent_world_item_inside_content",
          "button_mappings": [
            {
              "from_button_id": "button.menu_select",
              "to_button_id": "$pressed_button_name",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.menu_ok",
              "to_button_id": "$pressed_button_name",
              "mapping_type": "focused"
            },
            {
              "from_button_id": "button.controller_back",
              "to_button_id": "button.menu_start_local_world",
              "mapping_type": "global"
            }
          ],
          "bindings": [
            {
              "binding_type": "collection_details",
              "binding_collection_name": "local_worlds"
            },
            {
              "binding_name": "(#collection_index = 0)",
              "binding_name_override": "#enabled"
            }
          ],
          "controls": [
            {
              "default@$default_focus_border_control": {
                "size": "$border_size",
                "color": "$light_border_default_color"
              }
            },
            {
              "hover@$hover_focus_border_control": {
                "size": "$border_size",
                "color": "$light_border_hover_color"
              }
            },
            {
              "pressed@$pressed_focus_border_control": {
                "size": "$border_size",
                "color": "$light_border_pressed_color"
              }
            },
            {
              "content_inside@$content_inside": {
                "size": [ "100% - 2px", "100% - 2px" ]
              }
            }
          ]
        }
      }
    ]
  },

