  "local_world_folder_name@play.world_list_text": {
    "ignored": "(not ($win10_edition or $is_android or $is_ios))",
    "text": "#text",
    "color": "$secondary_text_color",
    "localize": false,
    "$image_path_cropped_property|default": "",
    "variables": [
      {
        "requires": "($win10_edition and not $pre_release)",
        "$image_path_cropped_property": "('§r' + (#image_path - ('%.90s' * #image_path)))"
      },
      {
        "requires": "($win10_edition and $pre_release)",
        "$image_path_cropped_property": "('§r' + (#image_path - ('%.98s' * #image_path)))"
      },
      {
        "requires": "$is_android",
        "$image_path_cropped_property": "(('§r' + #image_path) - '/storage/emulated/0/Android/data/com.mojang.minecraftpe/files/' - '/data/user/0/com.mojang.minecraftpe/')"
      },
      {
        "requires": "$is_ios",
        "$image_path_cropped_property": "(('§r' + #image_path) - ('%.94s' * ('§r' + #image_path))))"
        // path should be /var/mobile/Containers/Data/Application/<UUID, 36 characters total>/Documents/games/com.mojang
      }
    ],
    "bindings": [
      {
        "binding_name": "#local_world_image",
        "binding_name_override": "#image_path",
        "binding_type": "collection",
        "binding_collection_name": "local_worlds"
      },
      {
        "binding_type": "view",
        "source_property_name": "$image_path_cropped_property",
        "target_property_name": "#image_path_cropped"
      },
      {
        "binding_type": "view",
        "source_property_name": "('§r' + ((#image_path_cropped  - 'minecraftWorlds/' - 'LocalState/games/com.mojang/' - 'ocalState/games/com.mojang/' - 'calState/games/com.mojang/' - 'alState/games/com.mojang/' - 'lState/games/com.mojang/' - 'State/games/com.mojang/' - 'tate/games/com.mojang/' - 'ate/games/com.mojang/' - 'te/games/com.mojang/' - 'e/games/com.mojang/' - '/games/com.mojang/' - 'games/com.mojang/' - 'ames/com.mojang/' - 'mes/com.mojang/' - 'es/com.mojang/' - 's/com.mojang/' - '/com.mojang/' - 'com.mojang/' - 'om.mojang/' - 'm.mojang/') - '/world_icon.jpeg'))",
        "target_property_name": "#text"
      },
      {
        "binding_type": "view",
        "source_control_name": "show_world_folder_name_checkbox",
        "source_property_name": "#toggle_state",
        "target_property_name": "#checkbox_toggle_state"
      },
      {
        "binding_type": "view",
        "source_property_name": "((not ((('§r' + #image_path) - '/minecraftWorlds/') = ('§r' + #image_path))) and #checkbox_toggle_state)",
        "target_property_name": "#visible"
      }
    ]
  },

