  "ping_rate_panel/concurrency_stack/player_count": {
    "text": "#text",
    "bindings": [
      {
        "binding_type": "collection",
        "binding_name": "#server_player_count",
        "binding_name_override": "#c",
        "binding_collection_name": "$collection_name"
      },
      {
        "binding_type": "view",
        "source_property_name": "(('r' + #c) - ('r' + '%.1s' * #c + '/') - ('r' + '%.2s' * #c + '/') - ('r' + '%.3s' * #c + '/') - ('r' + '%.4s' * #c + '/') - ('r' + '%.5s' * #c + '/') - ('r' + '%.6s' * #c + '/') - ('r' + '%.7s' * #c + '/') - ('r' + '%.8s' * #c + '/') - ('r' + '%.9s' * #c + '/') - ('r' + '%.10s' * #c + '/') - 'r')",
        "target_property_name": "#max_c"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#c + 'r' - ('/' + #max_c + 'r'))",
        "target_property_name": "#cur_c"
      },
      {
        "binding_type": "view",
        "source_property_name": "(('§z' + #cur_c - 'r/' - 'r')",
        "target_property_name": "#text"
      }
    ]
  },

